<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LibCommonTitleBar">
        <attr name="ctb_title" format="string" />
        <attr name="ctb_titleColor" format="color" />
        <attr name="ctb_showBack" format="boolean" />
        <attr name="ctb_backRes" format="reference" />
        <attr name="ctb_rightText" format="string" />
        <attr name="ctb_rightIcon" format="reference" />
        <attr name="ctb_showLine" format="boolean" />
        <attr name="ctb_background" format="color" />
    </declare-styleable>

    <declare-styleable name="DatePickerView">
        <attr name="picker_item_background" format="reference" />
        <attr name="picker_divider_text" format="string" />
        <attr name="picker_divider_color" format="color" />
        <attr name="picker_mode" format="enum">
            <enum name="day" value="0" />
            <enum name="time" value="1" />
        </attr>
        <attr name="is_single" format="boolean" />
        <attr name="limit_days" format="integer" />
    </declare-styleable>

</resources>